ProyectoFinal:
	gcc /System/Library/Frameworks/GLUT.framework/GLUT /System/Library/Frameworks/OpenGL.framework/OpenGL main.cpp  -o ProyectoFinal
clean:
	rm ProyectoFinal
	gcc /System/Library/Frameworks/GLUT.framework/GLUT /System/Library/Frameworks/OpenGL.framework/OpenGL main.cpp  -o ProyectoFinal
run:
	rm ProyectoFinal
	gcc /System/Library/Frameworks/GLUT.framework/GLUT /System/Library/Frameworks/OpenGL.framework/OpenGL main.cpp  -o ProyectoFinal
	./ProyectoFinal
